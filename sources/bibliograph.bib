doi = {10.1002/bit.24914},
file = {:Users/MarkelovIgor/Documents/Mendeley papers/Holwerda, Lynd/2013 - Holwerda, Lynd - Testing Alternative Kinetic Models for Utilization of Crystalline Cellulose ( Avicel ) by Batch Cultures of Clos.pdf:pdf},
issn = {1097-0290},
journal = {Biotechnology and bioengineering},
keywords = {Anaerobiosis,Cellobiose,Cellobiose: metabolism,Cellulose,Cellulose: metabolism,Clostridium thermocellum,Clostridium thermocellum: metabolism,Fermentation,Kinetics,Models, Chemical},
mendeley-groups = {zz Markelov Igor/99 Bioenergetics/cellulose},
month = sep,
number = {9},
pages = {2389--94},
pmid = {23568291},
title = {{Testing alternative kinetic models for utilization of crystalline cellulose (Avicel) by batch cultures of Clostridium thermocellum.}},
