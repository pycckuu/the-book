isbn = {0092-8674 (Print)$\backslash$r0092-8674 (Linking)},
issn = {00928674},
journal = {Cell},
mendeley-groups = {zz Markelov Igor/After book Markov,zz Markelov Igor/99 Bioenergetics and Notes/Cannibalism,zz Markelov Igor/00 Finished readings/1505 May},
number = {3},
