issn = {1476-4687},
journal = {Nature},
keywords = {Anaerobiosis,Archaea,Archaea: classification,Archaea: metabolism,Bacteria,Bacteria: classification,Bacteria: metabolism,Bioreactors,Metagenome,Methane,Methane: metabolism,Nitrates,Nitrates: metabolism,Nitrites,Nitrites: metabolism,Nitrogen Cycle,Oxidation-Reduction,Quaternary Ammonium Compounds,Quaternary Ammonium Compounds: metabolism,Single-Cell Analysis,Transcriptome},
mendeley-groups = {zz Markelov Igor/After book Markov,zz Markelov Igor/99 Bioenergetics and Notes/CH4 Oxidation},
