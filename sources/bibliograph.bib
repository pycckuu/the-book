@article{Nakabachi2006,
abstract = {Previous studies have suggested that the minimal cellular genome could be as small as 400 kilobases. Here, we report the complete genome sequence of the psyllid symbiont Carsonella ruddii, which consists of a circular chromosome of 159,662 base pairs, averaging 16.5\% GC content. It is by far the smallest and most AT-rich bacterial genome yet characterized. The genome has a high coding density (97\%) with many overlapping genes and reduced gene length. Genes for translation and amino acid biosynthesis are relatively well represented, but numerous genes considered essential for life are missing, suggesting that Carsonella may have achieved organelle-like status.},
author = {Nakabachi, Atsushi and Yamashita, Atsushi and Toh, Hidehiro and Ishikawa, Hajime and Dunbar, Helen E and Moran, Nancy a and Hattori, Masahira},
