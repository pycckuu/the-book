month = apr,
number = {1},
pages = {124--34},
pmid = {22150269},
title = {{Persistence of bacterial proteolytic enzymes in lake ecosystems.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22150269},
volume = {80},
year = {2012}
}


