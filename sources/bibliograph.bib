issn = {1476-4687},
journal = {Nature},
keywords = {Carbon,Carbon Dioxide,Carbon Dioxide: metabolism,Carbon: metabolism,Greenhouse Effect,Plants,Plants: metabolism,Soil,Soil: analysis,Temperature},
